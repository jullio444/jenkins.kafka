{
	"filters": {
		"filterType": "all",
		"conditions": [
			{
				"field": "event.header.custom.partnerId",
				"value: "GOOGLE"
			},
			{
				"field": "event.header.custom.tenantId",
				"value": "MIDAS"
			}
		]
	},
	"conditions": {
		"applicationOperation": {
			"I": {
				"filterType": "all",
				"conditions": [
					{
						"field": "event.body.status",
						"value": "APPLICATION_RECEIVED"
					}
				]
			}
		},
	"applicationStatus": {
		"applications_approved": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.status",
					"value": "APPLICATION_APPROVED"
				}
			]
		},
		"applications_pended": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.status",
					"value": "APPLICATION_RECEIVED|APPLICATION_PENDED_ON_ADDITIONAL_DOCS|APPLICATION_PENDED_MANUAL_REVIEW|KBA_REQUESTED"
				}
			]
		},
		"applications_declined": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.status",
					"value": "APPLICATION_DECLINED"
				}
			]
		}
	},
	"previousApplicationStatus": {
		"applications_approved": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.previous_status",
					"value": "APPLICATION_APPROVED"
				}
			]
		},
		"applications_pended": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.previous_status",
					"value": "APPLICATION_RECEIVED|APPLICATION_PENDED_ON_ADDITIONAL_DOCS|APPLICATION_PENDED_MANUAL_REVIEW|KBA_REQUESTED"
				}
			]
		},
		"applications_declined": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.previous_status",
					"value": "APPLICATION_DECLINED"
				}
			]
		}
	},
	"accountStatus": {
		"accountOpen": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.status",
					"value": "OPENED"
				}
			]
		}
	},
	"accountType": {
		"accounts_savings": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.status",
					"value": "OPENED"
				},
				{
					"field": "event.body.type",
					"value": "071"
				},
				{
					"field": "event.body.class",
					"value": "009"
				}
			]
		},
		"accounts_checkings": {
			"filterType": "all",
			"conditions": [
				{
					"field": "event.body.status",
					"value": "OPENED"
				},
				{
					"field": "event.body.type",
					"value": "038"
				},
				{
					"field": "event.body.class",
					"value": "001"
				}
			]
		}
	}
}
}